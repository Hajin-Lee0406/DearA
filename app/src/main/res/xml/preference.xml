<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <PreferenceCategory
        app:iconSpaceReserved="false">
        <SwitchPreference
            app:key="LockSetting"
            app:title="암호"
            app:selectable="true"
            app:iconSpaceReserved="false">
        </SwitchPreference>
        <Preference
            app:key="LockChange"
            app:title="암호 변경"
            app:dependency="LockSetting"
            app:iconSpaceReserved="false">
        </Preference>
    </PreferenceCategory>

</PreferenceScreen>